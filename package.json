{
  "name": "@rubixstudios/payload-dub",
  "version": "1.0.0",
  "description": "A Payload CMS plugin that integrates with Dub to automatically create and manage shortlinks for your content.",
  "license": "MIT",
  "author": "Rubix Studios <hello@rubixstudios.com.au> (https://rubixstudios.com.au)",
  "homepage": "https://rubixstudios.com.au",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubix-studios-pty-ltd/payload-dub.git"
  },
  "bugs": {
    "url": "https://github.com/rubix-studios-pty-ltd/payload-dub/issues"
  },
  "maintainers": [
    {
      "name": "Rubix Studios",
      "email": "hello@rubixstudios.com.au",
      "url": "https://rubixstudios.com.au"
    }
  ],
  "type": "module",
  "keywords": [
    "payload",
    "payloadcms",
    "dub",
    "shortlinks",
    "plugin",
    "cms",
    "headless",
    "typescript",
    "react",
    "nextjs"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc -p tsconfig.json",
    "clean": "rimraf -g {dist,*.tsbuildinfo} && pnpm exec biome check --write",
    "update": "pnpm update --latest && pnpm install && pnpm prune && pnpm dedupe && pnpm run clean",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@payloadcms/eslint-config": "^3.28.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.10",
    "@types/react": "^19.2.9",
    "payload": "^3.73.0",
    "rimraf": "^6.1.2",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "payload": "^3.66.0"
  },
  "engines": {
    "node": ">=24",
    "pnpm": ">=10"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "main": "./dist/index.js",
    "provenance": true,
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "dependencies": {
    "dub": "^0.70.1"
  },
  "packageManager": "pnpm@10.28.2"
}
